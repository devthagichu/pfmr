<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="main.css" />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
      integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN"
      crossorigin="anonymous"
    />
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
<!-- include jQuery -->
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>
<!-- include Cycle2 -->
<script src="http://path/to/your/copy/of/jquery.cycle2.min.js"></script>
    <title>PFMR</title>
  </head>
  <body>
  {% if page.topnav %}
    <section>
      <nav class="nav bg-light">
        <div
          class="container d-flex justify-content-between align-items-center flex-wrap"
        >
<div class="d-flex">
{{ page.language }}

</div>

<div class="d-flex top-nav">
{{page.topnav }}
     
       <div
              class="social-media-icons d-flex justify-content-between align-items-center flex-wrap text-primary"
            >
              <i
                class="ml-2 fa-lg fa fa-facebook-official"
                aria-hidden="true"
              ></i>
              <i class="ml-2 fa-lg fa fa-twitter-square" aria-hidden="true"></i>
              <i class="ml-2 fa-lg fa fa-youtube-play" aria-hidden="true"></i>
              <i class="ml-2 fa-lg fa fa-instagram" aria-hidden="true"></i>
              <i class="ml-2 fa-lg fa fa-whatsapp" aria-hidden="true"></i>

</div>            </div>
        </div>
      </nav>
    </section>
 {% endif %}
   {% if page.menu %}
    <section class="sticky-top">
      <div class="sticky-top bg-white">
        <div class="container">
          {{page.header}}
        </div>
      </div>
      <nav class="navbar navbar-expand-lg navbar-dark bg-primary p-0">
        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="container">
          <div class="collapse navbar-collapse" id="navbarSupportedContent">

{{ page.menu }}
            <form class="form-inline my-2 my-lg-0">
              <!-- <input
                class="form-control mr-sm-2"
                type="search"
                placeholder="Search"
                aria-label="Search"
              /> -->
              <button
                class="btn btn-outline-success my-2 my-sm-0"
                type="submit"
              >
                <i class="fa fa-search text-white" aria-hidden="true"></i>
              </button>
            </form>
          </div>
        </div>
      </nav>
    </section>
     {% endif %}
   {% if page.hero %}
    <section>
      <div class="hero-bg">
        {{page.hero}}
        <div
          class="bg-primary hero-content d-flex justify-content-center align-items-center"
        >
          <div class="container">
            <div class="row">
              <div class="col-md-6">
                <h1 class="text-white">
                  Strengthening Governance For Enabling Service Delivery And
                  Economic Development Through Public Financial Management
                  Reforms(PFMR)
                </h1>
              </div>
              <div class="col-md-6">
{{page.slider }}
		</div>
            </div>
          </div>
        </div>
      </div>
      <div
        class="floating-icons d-flex flex-wrap container justify-content-between pt-2"
      >
      {% if page.floatingicons %}
        {{page.floatingicons}}
        {% endif %}
    </section>
       {% endif %}
   {% if page.welcome or page.visionmission %}
<section class="section">
    </section>

    <section class="section">
      <div class="container">
        <h5 class="mt-4 mb-4 font-weight-bolder text-primary">About</h5>
        <div class="row">
          <div class="col-md-6 welcome-message">
            {{page.welcome}}
          </div>
          <div class="col-md-6">
            {{page.visionmission}}
          </div>
        </div>
      </div>
    </section>
     {% endif %}
     {% if page.programmes %}
    <section class="section bg-light">
      <div class="container">
        <h5 class="mt-4 mb-4 font-weight-bolder text-primary">
          Our Programmes
        </h5>
        <div class="row pt-4">
          {{page.programmes}}
        </div>
      </div>
    </section>
{% endif %}
{% if page.featurednews or page.featuredevents %}
    <section class="section">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <h5 class="mt-4 mb-4 font-weight-bolder text-primary">News</h5>
            <div class="row">
              {{page.featurednews}}
              </div>
          </div>
          <div class="col-md-4">
            <h5 class="mt-4 mb-4 font-weight-bolder text-primary">Events</h5>
            {{page.featuredevents}}
          </div>
        </div>
      </div>
    </section>
    {% endif %}


    <div id="main-wrapper" class="layout-main-wrapper clearfix container">
      {% block content %}
        <div id="main" class="{{ container }}">
          {{ page.breadcrumb }}
          <div class="row row-offcanvas row-offcanvas-left clearfix">
            {% if page.sidebar_first %}
              <div{{ sidebar_first_attributes }} class="col-md-3" >
                <aside class="section" role="complementary">
                  {{ page.sidebar_first }}
                </aside>
              </div>
            {% endif %}
            {% if page.sidebar_second %}
              <div{{ sidebar_second_attributes }} class="col-md-3">
                <aside class="section" role="complementary">
                  {{ page.sidebar_second }}
                </aside>
              </div>
            {% endif %}
  <main{{ content_attributes }}>
                <section class="section">
                  <a id="main-content" tabindex="-1"></a>
                  {{ page.content }}
                </section>
              </main>


          </div>
        </div>
      {% endblock %}
    </div>        
   

   {% if page.stakeholders %}
    <section class="section bg-light">
      <div class="container">
        <h5 class="mt-4 mb-4 font-weight-bolder text-primary">
          Our Stakeholders
        </h5>
        <div class="row">
          {{page.stakeholders}}
        </div>
      </div>
    </section>
    {% endif %}
    {% if page.footer or page.copyright or page.termsandconditions %}
    <section class="section bg-primary text-white">
      {{page.footer}}
      <div
        class="container d-flex justify-content-between align-items-center flex-wrap"
      >
        {{page.copyright}}
        <p>
          Copyright Â© 2020. Public Financial Management Reforms Secretariat.
        </p>
        <div class="d-flex align-items-center">
          {{page.termsandconditions}}
          <p>
            <a href="#" class="text-decoration-none text-white"
              >Privacy Policies</a
            >
            |
            <a href="#" class="text-decoration-none text-white"
              >Terms of Use
            </a>
          </p>
          <img src="./assets/footer_logo.png" alt="" height="60px" />
        </div>
      </div>
    </section>
    {% endif %}

 
  </body>
</html>
